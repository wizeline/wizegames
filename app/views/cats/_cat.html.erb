<div id="<%= dom_id cat %>" class="cat_game">
  <% 3.times do |row| %>
    <div class='row cat_row_<%=row%>'>
      <% 3.times do |col| %>
      <span class='cell cat_col_<%=col%>'>
        <% unless cat.state['board'][row.to_s][col.to_s].nil? %>
          <%= cat.state['board'][row.to_s][col.to_s] %>
        <% else %>
          <% game = Game.last %>
          <%= button_to cat.state['current_symbol'], cat_moves_path(cat), params: { game_id: game.id, col: col, row: row } %>
        <% end %>
      </span>
      <% end %>
    </div>
  <% end %>

  <br><% cat.state.each do |element| %>
    <%= element %><br>
  <% end %><br>
</div>